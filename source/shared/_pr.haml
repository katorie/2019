- session = data.pr[id]
.timetable__section.timetable__section--session
  %ul.category
    %li.label.label--pr PR (#{session[:lang]})
    - if session[:twitter_id]
      %li @#{session[:twitter_id]}
  - if session[:title]
    %h3.title
      = session[:title]
  %ul.speaker
    - if session[:belongs]
      %li= session[:belongs]
    %li= session[:name]
